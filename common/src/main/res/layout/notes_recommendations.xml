<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true">

    <EditText
        android:id="@+id/editText5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textMultiLine"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="153dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="120dp"
        android:layout_height="39dp"
        android:background="@color/pinker"
        android:text="End Visit"
        android:textColor="@android:color/white"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="647dp" />

    <zm.gov.moh.common.submodule.form.widget.FormDatePickerWidget
        android:id="@+id/formDatePickerWidget"
        android:layout_width="161dp"
        android:layout_height="52dp"
        android:text="DD/MM/YYYY"
        tools:layout_editor_absoluteX="234dp"
        tools:layout_editor_absoluteY="153dp" />

    <zm.gov.moh.common.submodule.form.widget.FormDatePickerWidget
        android:id="@+id/formDatePickerWidget2"
        android:layout_width="161dp"
        android:layout_height="52dp"
        android:text="DD/MM/YYYY"
        tools:layout_editor_absoluteX="234dp"
        tools:layout_editor_absoluteY="247dp" />

    <zm.gov.moh.common.submodule.form.widget.FormDatePickerWidget
        android:id="@+id/formDatePickerWidget3"
        android:layout_width="161dp"
        android:layout_height="52dp"
        android:text="DD/MM/YYYY"
        tools:layout_editor_absoluteX="234dp"
        tools:layout_editor_absoluteY="358dp" />
</androidx.constraintlayout.widget.ConstraintLayout>